@model HashSet<string>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<form asp-controller="SortedSetType" asp-action="Add" method="post" enctype="application/x-www-form-urlencoded">
    <input  type="text" name="name" />
    <input  type="number" name="score" />
    <button class="btn btn-outline-primary" type="submit">Kaydet</button>
</form>

@foreach (var item in Model)
{
    <h1>@item <a class="btn btn-outline-danger" asp-controller="SortedSetType" asp-action="Delete" asp-route-name="@item">Sil</a></h1>
}

