@model Dictionary<string,string>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<form asp-controller="HashType" asp-action="Add" method="post" enctype="application/x-www-form-urlencoded">
    <input  type="text" name="name" />
    <input  type="text" name="value" />
    <button class="btn btn-outline-primary" type="submit">Kaydet</button>
</form>


@foreach (var item in Model)
{
    <h1>@item.Key => @item.Value <a class="btn btn-outline-danger" asp-controller="HashType" asp-action="Delete" asp-route-name="@item.Key">Sil</a></h1>
}